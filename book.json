{
    "title": "《k8s-1.13版本源码分析》",
    "author": "Tao Hu <farmer.hutao@outlook.com>",
    "description": "k8s source code analysis",
    "plugins": [
        "theme-comscore",
	"codeblock-filename",
        "navigator",
	"simple-page-toc",
	"expandable-chapters-small",
	"search-pro",
	"splitter",
	"multipart",
	"tbfed-pagefooter",
	"prism", "-highlight",
	"editlink",
	"github",
	"github-buttons@2.1.0",
	"donate",
	"copy-code-button"
    ],
    "pluginsConfig": {
	"theme-default": {
            "showLevel": true
        },
        "simple-page-toc": {
            "maxDepth": 4,
            "skipFirstH1": true
        },
	"editlink": {
            "base": "https://github.com/farmer-hutao/k8s-source-code-analysis/edit/master",
	    "label": "编辑本页",
            "multilingual": false
        },
	"github": {
            "url": "https://github.com/farmer-hutao/k8s-source-code-analysis"
        },
	"github-buttons": {
            "repo": "farmer-hutao/k8s-source-code-analysis",
            "types": [
                "star"
            ],
            "size": "small"
        },
	"donate": {
            "wechat": "./image/wx.jpg",
            "title": "催作者快快更新",
            "button": "鼓励作者",
            "wechatText": "微信小额捐赠，鼓励作者尽快更新～"
        },
	"search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord" : ["Gitbook Use"]
        },
	"tbfed-pagefooter": {
            "copyright":"Copyright &copy farmer.hutao@outlook.com 2019",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}
